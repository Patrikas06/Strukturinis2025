#include <iostream>
#include <string>
using namespace std;
int main()
{
  string Pavadinimas = "Vilniaus Rytas";
  int Ikurtas = 1997;
  string Savininkas = "Darius Gudelis, Vilniaus miesto savivaldybe";
  string Arena = "Jeep arena";
  int Talpa = 2500;
  
  
  cout << "Pavadinimas: "<<Pavadinimas<<endl;
  cout << "Ikurtas: "<<Ikurtas<<endl;
  cout << "Savininkas: "<<Savininkas<<endl;
  cout << "Arena: "<<Arena<<endl;
  cout << "Talpa: "<<Talpa<<endl;
  
  return 0;
}
